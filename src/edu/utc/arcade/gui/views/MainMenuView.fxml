<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<VBox alignment="CENTER" spacing="20.0" fx:controller="edu.utc.arcade.gui.controllers.MainMenuViewController"
      xmlns:fx="http://javafx.com/fxml">
    <Button fx:id="browseGamesButton" onAction="#handleSubmitButtonAction" prefWidth="350.0" minWidth="200"
            text="Browse Games"/>
    <Button fx:id="kioskModeButton" onAction="#handleSubmitButtonAction" prefWidth="350.0" minWidth="200"
            text="Kiosk Mode"/>
    <Button fx:id="updateButton" onAction="#handleSubmitButtonAction" prefWidth="350.0" minWidth="200" text="Update"/>
    <Button fx:id="exitButton" onAction="#handleSubmitButtonAction" prefWidth="350.0" minWidth="200" text="Exit"/>
</VBox>